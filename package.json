{
  "name": "rust-node-multi-template",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "log": "conventional-changelog --config ./node_modules/commitlint-cli/lib/log -i CHANGELOG.md -s -r 0",
    "commit": "npm run log && git add . && git-cz",
    "prepare": "husky install",
    "test": "vitest run"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/commitlint-cli/lib/cz"
    }
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^19.2.2",
    "@ls-lint/ls-lint": "^2.2.3",
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/shell-quote": "^1.7.5",
    "commitizen": "^4.3.0",
    "commitlint-cli": "^1.1.3",
    "conventional-changelog-cli": "^5.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.5",
    "oxlint": "^0.4.1",
    "prettier": "^3.2.5",
    "shell-quote": "^1.8.1",
    "turbo": "latest",
    "vitest": "^1.6.0"
  },
  "packageManager": "pnpm@8.9.0",
  "engines": {
    "node": ">=18"
  }
}
